spring:
  native:
    remove-yaml-support: true
  main:
    lazy-initialization: true
  aot:
    enabled: true
  application:
    name: GRPC Product
  datasource:
    url: jdbc:mysql://localhost:3306/grpc
    username: root
    password: password
    driver-class-name: com.mysql.cj.jdbc.Driver
  jpa:
    show-sql: true
    hibernate:
      ddl-auto: update

grpc:
  server:
    port: 9092

bucket:
  url: http://127.0.0.1:9000
  user: minioadmin
  key: minioadmin
  name: gabshop
  type: minio #change to 'local' if you don't want to use minio